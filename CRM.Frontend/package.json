{
  "name": "crm-frontend",
  "version": "0.0.24",
  "description": "CRM Frontend - Enterprise Customer Relationship Management System",
  "private": true,
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/lab": "^5.0.0-alpha.61",
    "@mui/material": "^5.14.15",
    "@mui/x-data-grid": "^7.3.0",
    "@react-oauth/google": "^0.12.1",
    "ajv": "^8.17.1",
    "axios": "^1.6.0",
    "formik": "^2.4.5",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.18.0",
    "recharts": "^2.10.3",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.37",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.15",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:analyze": "GENERATE_SOURCEMAP=true craco build && npx source-map-explorer 'build/static/js/*.js'",
    "build:fast": "GENERATE_SOURCEMAP=false CI=true craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "version:major": "node ../update-version.js major",
    "version:minor": "node ../update-version.js minor",
    "version:patch": "node ../update-version.js patch",
    "clean": "rm -rf build node_modules/.cache"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}